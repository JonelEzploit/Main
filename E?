            local A_1 = "Earth"
            local A_2 = {}
        
            local Event = game:GetService("ReplicatedStorage").Packages["_Index"]["sleitnick_knit@1.4.7"].knit.Services.EggService.RF.purchaseEgg

            Event:InvokeServer(A_1, A_2)
            wait(0.00000000000000000000001)
        end
        end        
